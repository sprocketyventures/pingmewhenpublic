		<array>
			<string>https://www.google.com/search*</string>
			<string>https://www.google.ca/search*</string>
			<string>https://duckduckgo.com/*</string>
			<string>https://www.bing.com/search*</string>
			<string>https://ca.search.yahoo.com/search*</string>
			<string>https://search.yahoo.com/search*</string>
		</array>
	</dict>
	<key>Description</key>
	<string>Enable PingMeWhen searches in other search engines</string>
	<key>ExtensionInfoDictionaryVersion</key>
	<string>1.0</string>
	<key>Permissions</key>
	<dict>
		<key>Website Access</key>
		<dict>
			<key>Include Secure Pages</key>
			<true/>
			<key>Level</key>
			<string>All</string>
		</dict>
	</dict>
	<key>Website</key>
	<string>http://www.pingmewhen.com</string>
